    "bootstrap-icons": "^1.13.1",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "deep-diff": "^1.0.2",
    "dotenv": "^16.4.7",
    "file-saver": "^2.0.5",
    "i18next-browser-languagedetector": "^8.0.2",
    "jspdf": "^3.0.1",
    "keycloak-js": "^26.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "papaparse": "^5.5.3",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.7",
    "react-data-table-component": "^7.6.2",
    "react-datepicker": "^8.4.0",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-quill": "^2.0.0",
    "react-redux": "^9.2.0",
    "react-router": "^7.1.1",
    "react-toastify": "^11.0.3",
    "sass": "1.72.0",
    "styled-components": "^6.1.14",
