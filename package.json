{
  "name": "project-templates",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://ahmadfaisall020597.github.io/testingjamkrida",
  "scripts": {
    "prepare-env-dev": "cp .env.dev .env",
    "prepare-env-qa": "cp .env.qa .env",
    "prepare-env-prd": "cp .env.prd .env",
    "dev": "vite --host --mode dev",
    "clean-dev": "rm -rf node_modules/.vite/ && vite --port 5173 --force --host --mode dev",
    "build": "vite build",
    "build-dev": "npm run prepare-env-dev && vite build --mode dev",
    "build-qa": "npm run prepare-env-qa && vite build --mode qa",
    "build-prd": "npm run prepare-env-prd && vite build --mode prd",
    "lint": "eslint .",
    "preview": "vite preview --port 5173",
    "prettier-write-all": "prettier --write .",
    "prettier-check-all": "prettier --check .",
    "predeploy": "npm run build-dev",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@react-keycloak/web": "^3.4.0",
    "@reduxjs/toolkit": "^2.5.0",
